/* component/top-fixed.wxss */
.top-fixed{
      width: 100%;
      height: 140rpx;
      position: fixed;
      top: 0;
      left: 0;
      z-index: 9999;
      background-color: #fff;
      transition: 0.6s;
      box-shadow: 1px 1px 2px #ccc;
}
.top-fixed navigator{
      width: 48rpx;
      height: 48rpx;
      position: absolute;
      left: 30rpx;
      top: 80rpx;
      background: url("http://192.168.1.110:8080/platform/statics/img/goBack.png") center / 48rpx 48rpx no-repeat;
}                                